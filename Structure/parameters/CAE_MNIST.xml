<DCAE>
	<Layer>
		<type>Convolution2D</type>
		<kernel_shape>5,5,1,32</kernel_shape>
		<activation>relu</activation>
		<strides>1,1,1,1</strides>
		<padding>SAME</padding>
		<scope>conv_1</scope>
		<input>x</input>
	</Layer>
	
	<Layer>
		<type>MaxPooling</type>
		<kernel_shape>1,2,2,1</kernel_shape>
		<strides>1,2,2,1</strides>
		<padding>SAME</padding>
		<scope>pool_1</scope>
		<input>conv_1</input>
	</Layer>
	
	<Layer>
		<type>Convolution2D</type>
		<kernel_shape>5,5,32,32</kernel_shape>
		<strides>1,1,1,1</strides>
		<padding>SAME</padding>
		<activation>relu</activation>
		<scope>conv_2</scope>
		<input>pool_1</input>
	</Layer>
	
	<Layer>
		<type>MaxPooling</type>
		<kernel_shape>1,2,2,1</kernel_shape>
		<strides>1,2,2,1</strides>
		<padding>SAME</padding>
		<scope>pool_2</scope>
		<input>conv_2</input>
	</Layer>
	
	<Layer>
		<type>Unfold</type>
		<scope>unfold</scope>
		<input>pool_2</input>
	</Layer>
	
	<Layer>
		<type>FullyConnected</type>
		<output_dim>20</output_dim>
		<activation>relu</activation>
		<scope>encode</scope>
		<input>unfold</input>
	</Layer>
	
	<Layer>
		<type>FullyConnected</type>
		<output_dim>7*7*32</output_dim>
		<activation>relu</activation>
		<scope>decode</scope>
		<input>encode</input>
	</Layer>
	
	<Layer>
		<type>Fold</type>
		<fold_shape>-1,7,7,32</fold_shape>
		<scope>fold</scope>
		<input>decode</input>
	</Layer>
	
	<Layer>
		<type>UnPooling</type>
		<kernel_shape>2,2</kernel_shape>
		<output_shape>conv_2</output_shape>
		<scope>unpool_1</scope>
		<input>fold</input>
	</Layer>
	
	<Layer>
		<type>DeConvolution2D</type>
		<kernel_shape>5,5,32,32</kernel_shape>
		<output_shape>pool_1</output_shape>
		<activation>relu</activation>
		<scope>deconv_1</scope>
		<input>unpool_1</input>
	</Layer>
	
	<Layer>
		<type>UnPooling</type>
		<kernel_shape>2,2</kernel_shape>
		<output_shape>conv_1</output_shape>
		<scope>unpool_2</scope>
		<input>deconv_1</input>
	</Layer>
	
	<Layer>
		<type>DeConvolution2D</type>
		<kernel_shape>5,5,1,32</kernel_shape>
		<output_shape>x</output_shape>
		<activation>sigmoid</activation>
		<scope>deconv_2</scope>
		<input>unpool_2</input>
	</Layer>
	
</DCAE>